<h1>Show</h1>
<div>

<label>Title: <%= @post.title %> </label><br>
<label>Content: <%= @post.content %></label><br>
<label>Likes: <%= @post.likes%></label><br>
<%= link_to @post.blogger.name, blogger_path(@post.blogger) %> 
<%= link_to @post.destination.name, destination_path(@post.destination) %>
<%= link_to "Like", like_path(@post) %> 

</div><br>


<%= link_to "Edit??", edit_post_path(@post) %>
<%= link_to "Index??", posts_path %>

